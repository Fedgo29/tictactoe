{"ast":null,"code":"import \"core-js/modules/esnext.iterator.constructor.js\";\nimport \"core-js/modules/esnext.iterator.every.js\";\nimport { ref, computed } from \"vue\";\nimport AppHeader from './components/Header.vue';\nexport default {\n  name: \"TicTacToe\",\n  components: {\n    AppHeader\n  },\n  setup() {\n    const board = ref(Array(9).fill(null));\n    const currentPlayer = ref(\"X\");\n    const winner = ref(null);\n    const winnerMessage = computed(() => {\n      return winner.value ? `ยก${winner.value} gana!` : \"\";\n    });\n    const winningCombinations = [[0, 1, 2], [3, 4, 5], [6, 7, 8], [0, 3, 6], [1, 4, 7], [2, 5, 8], [0, 4, 8], [2, 4, 6]];\n    const checkWinner = () => {\n      for (const combination of winningCombinations) {\n        const [a, b, c] = combination;\n        if (board.value[a] && board.value[a] === board.value[b] && board.value[a] === board.value[c]) {\n          winner.value = board.value[a];\n          return;\n        }\n      }\n      if (board.value.every(cell => cell)) {\n        winner.value = \"Empate\";\n      }\n    };\n    const makeMove = index => {\n      if (!board.value[index] && !winner.value) {\n        board.value[index] = currentPlayer.value;\n        checkWinner();\n        currentPlayer.value = currentPlayer.value === \"X\" ? \"O\" : \"X\";\n      }\n    };\n    const resetGame = () => {\n      board.value = Array(9).fill(null);\n      currentPlayer.value = \"X\";\n      winner.value = null;\n    };\n    return {\n      board,\n      currentPlayer,\n      winner,\n      winnerMessage,\n      makeMove,\n      resetGame\n    };\n  }\n};","map":{"version":3,"names":["ref","computed","AppHeader","name","components","setup","board","Array","fill","currentPlayer","winner","winnerMessage","value","winningCombinations","checkWinner","combination","a","b","c","every","cell","makeMove","index","resetGame"],"sources":["C:\\Users\\Edgar\\Documents\\GitHub\\tictactoe\\tictactoe\\src\\App.vue"],"sourcesContent":["<template>\n  <div id=\"app\" class=\"game\">\n    <AppHeader />\n    <div class=\"board\">\n      <div\n        v-for=\"(cell, index) in board\"\n        :key=\"index\"\n        class=\"cell\"\n        :class=\"{ 'cell-x': cell === 'X', 'cell-o': cell === 'O' }\"\n        @click=\"makeMove(index)\"\n      >\n        {{ cell }}\n      </div>\n    </div>\n    <div v-if=\"winner\" class=\"winner\">\n      <p>{{ winnerMessage }}</p>\n      <button @click=\"resetGame\">Reiniciar Juego</button>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { ref, computed } from \"vue\";\nimport AppHeader from './components/Header.vue';\n\nexport default {\n  name: \"TicTacToe\",\n  components: {\n    AppHeader,\n  },\n  setup() {\n    const board = ref(Array(9).fill(null));\n    const currentPlayer = ref(\"X\");\n    const winner = ref(null);\n\n    const winnerMessage = computed(() => {\n      return winner.value ? `ยก${winner.value} gana!` : \"\";\n    });\n\n    const winningCombinations = [\n      [0, 1, 2],\n      [3, 4, 5],\n      [6, 7, 8],\n      [0, 3, 6],\n      [1, 4, 7],\n      [2, 5, 8],\n      [0, 4, 8],\n      [2, 4, 6],\n    ];\n\n    const checkWinner = () => {\n      for (const combination of winningCombinations) {\n        const [a, b, c] = combination;\n        if (\n          board.value[a] &&\n          board.value[a] === board.value[b] &&\n          board.value[a] === board.value[c]\n        ) {\n          winner.value = board.value[a];\n          return;\n        }\n      }\n      if (board.value.every((cell) => cell)) {\n        winner.value = \"Empate\";\n      }\n    };\n\n    const makeMove = (index) => {\n      if (!board.value[index] && !winner.value) {\n        board.value[index] = currentPlayer.value;\n        checkWinner();\n        currentPlayer.value = currentPlayer.value === \"X\" ? \"O\" : \"X\";\n      }\n    };\n\n    const resetGame = () => {\n      board.value = Array(9).fill(null);\n      currentPlayer.value = \"X\";\n      winner.value = null;\n    };\n\n    return {\n      board,\n      currentPlayer,\n      winner,\n      winnerMessage,\n      makeMove,\n      resetGame,\n    };\n  },\n};\n</script>\n\n<style>\n* {\n  box-sizing: border-box;\n}\nhtml, body {\n  margin: 0;\n  padding: 0;\n\n}\n.game {\n  font-family: Arial, sans-serif;\n  text-align: center;\n  margin-top: 50px;\n}\n\n.board {\n  display: grid;\n  grid-template-columns: repeat(3, 100px);\n  grid-gap: 5px;\n  justify-content: center;\n  margin: 20px auto;\n}\n\n.cell {\n  width: 100px;\n  height: 100px;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  border: 2px solid #444;\n  font-size: 24px;\n  font-weight: bold;\n  cursor: pointer;\n  background-color: #fff;\n}\n\n.cell-x {\n  color: #2196f3;\n}\n\n.cell-o {\n  color: #f44336;\n}\n\n.winner {\n  margin-top: 20px;\n}\n</style>\n"],"mappings":";;AAsBA,SAASA,GAAG,EAAEC,QAAO,QAAS,KAAK;AACnC,OAAOC,SAAQ,MAAO,yBAAyB;AAE/C,eAAe;EACbC,IAAI,EAAE,WAAW;EACjBC,UAAU,EAAE;IACVF;EACF,CAAC;EACDG,KAAKA,CAAA,EAAG;IACN,MAAMC,KAAI,GAAIN,GAAG,CAACO,KAAK,CAAC,CAAC,CAAC,CAACC,IAAI,CAAC,IAAI,CAAC,CAAC;IACtC,MAAMC,aAAY,GAAIT,GAAG,CAAC,GAAG,CAAC;IAC9B,MAAMU,MAAK,GAAIV,GAAG,CAAC,IAAI,CAAC;IAExB,MAAMW,aAAY,GAAIV,QAAQ,CAAC,MAAM;MACnC,OAAOS,MAAM,CAACE,KAAI,GAAI,IAAIF,MAAM,CAACE,KAAK,QAAO,GAAI,EAAE;IACrD,CAAC,CAAC;IAEF,MAAMC,mBAAkB,GAAI,CAC1B,CAAC,CAAC,EAAE,CAAC,EAAE,CAAC,CAAC,EACT,CAAC,CAAC,EAAE,CAAC,EAAE,CAAC,CAAC,EACT,CAAC,CAAC,EAAE,CAAC,EAAE,CAAC,CAAC,EACT,CAAC,CAAC,EAAE,CAAC,EAAE,CAAC,CAAC,EACT,CAAC,CAAC,EAAE,CAAC,EAAE,CAAC,CAAC,EACT,CAAC,CAAC,EAAE,CAAC,EAAE,CAAC,CAAC,EACT,CAAC,CAAC,EAAE,CAAC,EAAE,CAAC,CAAC,EACT,CAAC,CAAC,EAAE,CAAC,EAAE,CAAC,CAAC,CACV;IAED,MAAMC,WAAU,GAAIA,CAAA,KAAM;MACxB,KAAK,MAAMC,WAAU,IAAKF,mBAAmB,EAAE;QAC7C,MAAM,CAACG,CAAC,EAAEC,CAAC,EAAEC,CAAC,IAAIH,WAAW;QAC7B,IACET,KAAK,CAACM,KAAK,CAACI,CAAC,KACbV,KAAK,CAACM,KAAK,CAACI,CAAC,MAAMV,KAAK,CAACM,KAAK,CAACK,CAAC,KAChCX,KAAK,CAACM,KAAK,CAACI,CAAC,MAAMV,KAAK,CAACM,KAAK,CAACM,CAAC,GAChC;UACAR,MAAM,CAACE,KAAI,GAAIN,KAAK,CA